language: python  
python:  
  - 3.5
install:  
  - pip3.5 install python-coveralls
  - pip3.5 install coverage
  - pip3.5 install -r requirements.txt
script:
  - coverage run -a shared/iftools.py
  - coverage run -a shared/api_wrapper.py
  - coverage run -a shared/dnstools.py
  - coverage report
after_success:  
  - coveralls